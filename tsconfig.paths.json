{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@api/*": ["api/*"],
      "@core/*": ["core/*"],
      "@hooks/*": ["hooks/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@pages/*": ["pages/*"],
      "@routes/*": ["routes/*"],
      "@styles/*": ["styles/*"],
      "@constants/*": ["constants/*"],
      "@components/*": ["components/*"]
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
